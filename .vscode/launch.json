    "version": "0.2.0",
    "configurations": [
        {
            "name": "vae",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/darknet",
            "args": [
                "vae",
                "train",
                "vae_connected.cfg",
                "mnist_train_vae.csv",
                "-N",
                "4000"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "autoencoder",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/darknet",
            "args": [
                "autoencoder",
                "autoencoder.cfg",
                "mnist_train.csv",
                "1000"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
